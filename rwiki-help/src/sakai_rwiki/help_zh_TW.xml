      <value>建立新 wiki 頁面</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/creating_pages_zh_TW.htm</value>
    </property>
  </bean>
  <bean id="wikiContent5" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content5_zh_TW</value>
    </property>
    <property name="name">
      <value>新增影像和附件</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/adding_images_zh_TW.htm</value>
    </property>
  </bean>
  <bean id="wikiContent6" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content6_zh_TW</value>
    </property>
    <property name="name">
      <value>檢視頁面資訊</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/viewing_info_zh_TW.htm</value>
