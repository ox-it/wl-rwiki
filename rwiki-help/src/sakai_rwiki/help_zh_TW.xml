<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="wikiContent1" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content1_zh_TW</value>
    </property>
    <property name="name">
      <value>Wiki：概述</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/overview_zh_TW.htm</value>
    </property>
    <property name="defaultForTool"><value>sakai.rwiki</value></property>

  </bean>
  <bean id="wikiContent2" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content2_zh_TW</value>
    </property>
    <property name="name">
      <value>檢視頁面</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/viewing_pages_zh_TW.htm</value>
